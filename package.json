{
  "name": "ng-statusreport",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --host 0.0.0.0 --port 4200",
    "dev-build": "npm run config -- && ng build",
    "prod-build": "npm run config -- && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer && gulp compress",
    "build": "ng build --prod --aot",
    "config": "node ./config-env.js",
    "start": "node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run prod-build",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}"
  },
  "private": true,
  "dependencies": {
    "@angular-buddies/prettier": "^1.0.0-alpha.3",
    "@angular-devkit/core": "^9.0.1",
    "@angular-devkit/schematics": "^9.0.1",
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "~11.0.1",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/elements": "^11.0.2",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@angular/service-worker": "^11.0.2",
    "@fortawesome/fontawesome-pro": "^5.12.1",
    "compression": "^1.7.4",
