    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.10.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
