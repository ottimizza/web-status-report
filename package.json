    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~4.0.5",
    "prettier": "^1.12.1",
    "lint-staged": "^7.1.2",
    "husky": "^0.14.3"
